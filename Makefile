DIR=src

all:
	cd $(DIR) && $(MAKE)


.PHONY: clean tags

clean:
	cd $(DIR) && ./switch-debug-production --debug && $(MAKE) clean

tags:
	cd $(DIR) && ./switch-debug-production --debug && $(MAKE) tags
